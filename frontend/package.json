{
  "name": "yukichi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "APP_ENV=local next dev",
    "dev": "APP_ENV=develop next dev",
    "build:develop": "APP_ENV=develop next build",
    "build:release": "APP_ENV=release next build",
    "build": "NODE_OPTIONS=--max_old_space_size=6144 next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write . './**/*.{js,jsx,ts,tsx,json,css}'",
    "figmaToken": "npx token-transformer designTokens/figma-token.json designTokens/token.json global",
    "generate-client": "openapi-generator-cli generate -i ./open_api.yml -g typescript-axios -o ./openapi",
    "clone-yukichi-api": "git clone git@github.com:yuko-dev/yukichi-api.git",
    "copy-openapi-file": "cp yukichi-api/api/open_api.yml ./",
    "delete-yukichi-api": "rm -rf yukichi-api",
    "generate-openapi": "yarn clone-yukichi-api && yarn copy-openapi-file && yarn delete-yukichi-api && yarn generate-client"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^2.16.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@lexical/react": "^0.11.1",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.11.0",
    "@mui/x-date-pickers": "^5.0.3",
    "aws-amplify": "^4.3.20",
    "aws-jwt-verify": "^4.0.0",
    "dayjs": "^1.11.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lexical": "^0.11.1",
    "material-ui-popup-state": "^5.0.9",
    "next": "^12.3.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-loading-icons": "^1.0.8",
    "react-outside-click-handler": "^1.3.0",
    "react-rnd": "^10.4.1",
    "react-window": "^1.8.9",
    "react-zoom-pan-pinch": "^2.1.3",
    "recoil": "^0.7.6",
    "swiper": "^8.4.3",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@svgr/webpack": "^8.1.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^18.18.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.50.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.7",
    "next-transpile-modules": "^9.0.0",
    "openapi-typescript": "^6.1.0",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.2.1",
    "token-transformer": "^0.0.19",
    "type-fest": "^3.5.1",
    "typescript": "^5.2.2"
  }
}
